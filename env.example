# Configuración del servidor
PORT=8080

# Configuración de imágenes
MAX_IMAGE_SIZE=33554432
MAX_BATCH_SIZE=10

# Configuración de logging
LOG_LEVEL=info

# Configuración de Swagger
SWAGGER_HOST=localhost:8080
SWAGGER_SCHEME=http

# Configuración de compresión por defecto
DEFAULT_QUALITY=80
DEFAULT_FORMAT=jpeg

# Configuración de timeouts (en segundos)
REQUEST_TIMEOUT=60
UPLOAD_TIMEOUT=300
